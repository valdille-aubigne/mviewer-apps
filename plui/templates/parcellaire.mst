<script>

	var i, j, k, l, x ="<ul class=\"liste\">",y="<ul class=\"liste\">", z="<ul class=\"liste\">", aa="<ul class=\"liste\">";

	var idu=[];
	{{#features}}
		idu.push('{{idu}}');
	{{/features}}

	idu=idu.toString();
	idu = "Parcelle n°" + idu.substring(6,8).replace(/^0{1}/,'') + " " + idu.substring(8,12).replace(/^0{1,}/,'');
	document.getElementById("numparc").innerHTML = idu;

	var zonage=[];
	{{#features}}
		zonage.push('{{zonage}}');
	{{/features}}

	zonage=zonage.toString();
	zonage=zonage.replace(/&quot;/g, '"');
	zonage = JSON.parse(zonage);

	for (i = 0; i < zonage.length; i++) {
  x += "<li>" + zonage[i]["libelle"] + " - " + zonage[i]["libelong"] + " (" +
			"<a target=\"_blank\"  href=" + zonage[i]["urlfic"] + ">Ouvrir le pdf)</a></li>";
}
x += "</ul>";
document.getElementById("zonlist").innerHTML = x;

var prescription=[];
	{{#features}}
		prescription.push('{{prescription}}');
	{{/features}}

if (prescription != '') {

	prescription=prescription.toString();
	prescription=prescription.replace(/&quot;/g, '"');
	prescription = JSON.parse(prescription);

for (j = 0; j < prescription.length; j++) {
  y += "<li>" + prescription[j]["libelle"] ;
if (prescription[j]["txt"] != null){
  y += " : " + prescription[j]["txt"];
}
if (prescription[j]["libtypepsc"] != null){
  y += " - " + prescription[j]["libtypepsc"] ;
}
if (prescription[j]["piece_ecrite"] != null){
  y += " (" +	"<a target=\"_blank\"  href=" + prescription[j]["piece_ecrite"] + ">Ouvrir le pdf)</a>";
}
  y += "</li>" ;
}
}else{
y += "<li>Aucune prescription sur la parcelle</li>";
}
y += "</ul>";
document.getElementById("psclist").innerHTML = y;

var information=[];
	{{#features}}
		information.push('{{information}}');
	{{/features}}

if (information != '') {

	information=information.toString();
	information=information.replace(/&quot;/g, '"');
	information = JSON.parse(information);

for (k = 0; k < information.length; k++) {
 if (!(information[k]["typeinf"] == '19' && information[k]["stypeinf"] == '01') || (information[k]["libelle"] == 'Zonage assainissement' )){
 z += "<li>" + information[k]["libelle"];

if (information[k]["txt"] != null){ 
  z += " : " + information[k]["txt"];
}
//if (information[k]["libtypeinf"] != null){
 // z += " - " + information[k]["libtypeinf"] ;
//}
if (information[k]["urlfic"] != null){
  z += " (" +	"<a target=\"_blank\"  href=" + information[k]["urlfic"] + ">Ouvrir)</a>";
} 
  z += "</li>" ;
}
}}else{
z += "<li>Aucune contraintes autre sur la parcelle</li>";
}
z += "</ul>";
document.getElementById("inflist").innerHTML = z;


var sup=[];
	{{#features}}
		sup.push('{{sup}}');
	{{/features}}

if (sup != '') {

	sup=sup.toString();
	sup=sup.replace(/&quot;/g, '"');
	sup = JSON.parse(sup);

for (l = 0; l < sup.length; l++) {
  aa += "<li>" + sup[l]["libelle"];

if (sup[l]["urlfic"] != null){
  aa += " (" +	"<a target=\"_blank\"  href=" + sup[l]["urlfic"] + ">Ouvrir le pdf)</a>";
}
  aa += "</li>" ;
}
}
aa += "<li>T7 : Extérieur des zones de dégagement</li>";
aa += "</ul>";
document.getElementById("suplist").innerHTML = aa;

</script>
