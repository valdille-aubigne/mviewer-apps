
{{#features}}

<div id="numparc" class="title-feature" style="padding-top:8px;"></div>

		<div class = "item active" style="margin-top:-2%;">
			<div class="container" style="padding-left:0!important;margin-left:0!important;margin-bottom:10%;overflow:hidden;">
				<div class="row">
					<div class="col-xs-3" style="padding-right:0px; padding-left:5px; margin-left:1%; margin-top:0%;">
						<div class="title-number" style="text-align:left;">
							<p>
							<span style="font-family:'roboto_bold'"> Dispositions générales :</span>
							<span style="font-size:12px; text-align:left; font-family:'roboto_regular'; color:#777;">
							<a target=\"_blank\"  href="https://www.valdille-aubigne.fr/wp-content/uploads/2020/PLUI/4.1_reglement_litteral_approbation.pdf#page=8">Ouvrir le pdf</a>
							</span>
							</p>
						</div>
					</div>
					<div class="col-xs-8" style="padding-right:0px; padding-left:5px; margin-left:1%; margin-top:0%;">
						<div class="title-number" style="text-align:left;">
							<p>
							<span style="font-family:'roboto_bold'"> OAP thématiques :</span>
							<span style="font-size:12px; text-align:left; font-family:'roboto_regular'; color:#777;">
							<a target=\"_blank\"  href="https://www.valdille-aubigne.fr/wp-content/uploads/2020/PLUI/3.Orientation_amenagement/3.1_OAP_thematiques/3.1.3_OAP_Trame_verte_bleue_approbation.pdf">OAP Trame verte et bleue </a> /
							<a target=\"_blank\"  href="https://www.valdille-aubigne.fr/wp-content/uploads/2020/PLUI/3.Orientation_amenagement/3.1_OAP_thematiques/3.1.2_OAP_Patrimoine_approbation.pdf">OAP Patrimoine et paysage </a> /
							<a target=\"_blank\"  href="https://www.valdille-aubigne.fr/wp-content/uploads/2020/PLUI/3.Orientation_amenagement/3.1_OAP_thematiques/3.1.1_OAP_Commerce_approbation.pdf">OAP Commerce </a>
							</span>
							</p>
						</div>
					</div>
				</div>
				<div class="row">
					<!-- ZONAGE -->
					<div class="col-xs-11" style="padding-right:0px; padding-left:5px; margin-left:1%; margin-top:0%;">
						<div class="title-number" style="text-align:left;">
							<p>
							<span style="font-family:'roboto_bold'"> Zonage(s) :</span><br/>
							<span id="zonlist" style="font-size:12px; text-align:left; font-family:'roboto_regular'; color:#777;"></span>
							</p>
						</div>
					</div>
				</div>	
				<div class="row">
					<!-- PRESCRIPTION -->
					<div class="col-xs-5" style="padding-right:0px; padding-left:5px; margin-left:1%; margin-top:0%;">
						<div class="title-number" style="text-align:left;">
							<p>
							<span style="font-family:'roboto_bold'"> Prescription(s) :</span><br/>
							<span id="psclist" style="font-size:12px; text-align:left; font-family:'roboto_regular'; color:#777;"></span>
							</p>
						</div>
					</div>

					<!-- SUP -->
					<div class="col-xs-3" style="padding-right:0px; padding-left:5px; margin-left:1%; margin-top:0%;">
						<div class="title-number" style="text-align:left;">
							<p>
							<span style="font-family:'roboto_bold'"> Servitude(s) :</span><br/>
							<span id="suplist" style="font-size:12px; text-align:left; font-family:'roboto_regular'; color:#777;"></span>
							</p>
						</div>
					</div>

					<!-- INFORMATION -->
					<div class="col-xs-3" style="padding-right:0px; padding-left:5px; margin-left:1%; margin-top:0%;">
						<div class="title-number" style="text-align:left;">
							<p>
							<span style="font-family:'roboto_bold'"> Annexe(s) :</span><br/>
							<span id="inflist" style="font-size:12px; text-align:left; font-family:'roboto_regular'; color:#777;">
							</span>
							</p>
						</div>
					</div>

				</div>
			</div>
		</div>


