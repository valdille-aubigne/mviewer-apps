			{{#araigne}}<span style="font-family:'roboto_bold'"> Araignées :</span> {{araigne}}<br/>{{/araigne}}
			{{#ainsecte}}<span style="font-family:'roboto_bold'"> Autres insectes :</span> {{ainsecte}}<br/>{{/ainsecte}}
			{{#autrefaune}}<span style="font-family:'roboto_bold'"> Autre :</span> {{autrefaune}}<br/>{{/autrefaune}}
			</br>
		</p>
	</li>
{{/features}}

