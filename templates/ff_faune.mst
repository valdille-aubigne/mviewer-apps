
{{#features}}
	<li class="item" style="width:238px;">
		<h3 class="title-feature">Faune</h3>

		<p class="text-feature">		
			{{#faune}}<span style="font-family:'roboto_bold'"> Observations faunistiques :</span> {{faune}}<br/>{{/faune}}
			{{#oiseaux}}<span style="font-family:'roboto_bold'"> Oiseaux :</span> {{oiseaux}}<br/>{{/oiseaux}}
			{{#papillons}}<span style="font-family:'roboto_bold'"> Papillons :</span> {{papillons}}<br/>{{/papillons}}
			{{#reptiles}}<span style="font-family:'roboto_bold'"> Reptiles :</span> {{reptiles}}<br/>{{/reptiles}}
			
