			{{#libellules}}<span style="font-family:'roboto_bold'"> Libellules :</span> {{libellules}}<br/>{{/libellules}}
			{{#amphibien}}<span style="font-family:'roboto_bold'"> Amphibiens :</span> {{amphibien}}<br/>{{/amphibien}}
